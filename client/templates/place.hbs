              <div class="d-flex align-items-center">
                <div class="icon"><i class="fa fa-check"></i></div><span>Elevator</span>
              </div>
            </li>
            <li class="list-inline-item">
              <div class="d-flex align-items-center">
                <div class="icon"><i class="fa fa-check"></i></div><span>Vegan friendly</span>
              </div>
            </li>
            <li class="list-inline-item">
              <div class="d-flex align-items-center">
                <div class="icon"><i class="fa fa-check"></i></div><span>Alcohol served</span>
              </div>
            </li>
