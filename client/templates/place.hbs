      </main>
      <!-- Opening Hours      -->
      <aside class="col-lg-4">
        <div class="widget opening-hours">
          <header>
            <h3 class="has-lines"><small>Open Status</small> Opening Hours </h3>
          </header>
          <div class="days">
          </div>
        </div>
        <!-- Contact Info-->
        <div class="widget contact">
          <header>
            <h3 class="has-lines"><small>Contact</small> Drop Them A Line</h3>
          </header>
          <div class="info">
            <div class="item"><a id="phone" href="#"><i class="fa fa-phone"></i> </a></div>
            <div class="item"><a id="email" href=""><i class="fa fa-envelope-o"></i> </a></div>
            <div class="item"><a id="website" href="#"><i class="fa fa-globe"></i> </a></div>
            <div class="item"><a id="facebook" href="#" target="_blank" class="facebook"><i class="fa fa-facebook-official"></i>
                Facebook</a></div>
            <div class="item"><a id="twitter" href="#" target="_blank" class="twitter"><i class="fa fa-twitter-square"></i>
                Twitter</a></div>
            <div class="item"><a id="instagram" href="#" target="_blank" class="instagram"><i class="fa fa-instagram"></i>
                Instagram</a></div>
          </div>
        </div>
      </aside>
    </div>
  </div>
  <!-- Page Footer-->
  {{> ./partials/footer}}
