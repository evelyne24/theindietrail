<!DOCTYPE html>
<html>

{{> ./partials/head}}

<body>
  {{> ./partials/header basename=""}}
  <div class="listing-categories-page">
    <!-- Hero Section-->
    <section class="hero hero-inverse">
      <div class="container">
        <h1>{{place}}</h1>
        <p class="lead">{{description}}</p>
      </div>
    </section>
    <section class="no-padding-top padding-small">
      <div class="block has-shadow">
        <div id="category-map" class="map-holder"></div>
      </div>
      <div class="container">
        <p id="places-count" class="lead text-muted block text-center"><strong>{{number}}</strong> top places to visit</p>
        <div id="items" class="row block"></div>
      </div>
    </section>
  </div>
  <!-- Page Footer-->
  {{> ./partials/footer}}
  <!-- JavaScript files-->
  <!-- create your own Maps API Key for production use, this one is domain-restricted-->
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCRVq-3F_QrSVTgFthNRqDCD-CoUWAKkFU"></script>
</body>

</html>